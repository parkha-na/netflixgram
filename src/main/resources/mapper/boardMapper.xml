<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.parkhana.mapper.BoardMapper">
    <select id="selectBoardGameList" resultType="com.github.parkhana.data.Board">
        <![CDATA[
        SELECT
            id
            , contents
            , nickname
            , uploaddate
            , hits
            , recommend
        FROM board
        WHERE 1=1
        ORDER BY id;
        ]]>
    </select>
</mapper>